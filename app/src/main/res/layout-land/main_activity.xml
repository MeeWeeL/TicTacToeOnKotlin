<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.MainActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/back_white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/status"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:gravity="center"
        android:textSize="30sp"
        android:textStyle="bold"
        android:text="You Win!!!"
        android:textColor="#8A2BE2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/position_02"
        app:layout_constraintVertical_weight="1"
        android:visibility="gone"/>
    <ImageButton
        android:id="@+id/position_00"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/status"
        app:layout_constraintBottom_toTopOf="@+id/position_10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/position_01"/>

    <ImageButton
        android:id="@+id/position_01"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/status"
        app:layout_constraintBottom_toTopOf="@+id/position_11"
        app:layout_constraintStart_toEndOf="@id/position_00"
        app:layout_constraintEnd_toStartOf="@+id/position_02"/>

    <ImageButton
        android:id="@+id/position_02"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/status"
        app:layout_constraintBottom_toTopOf="@+id/position_12"
        app:layout_constraintStart_toEndOf="@id/position_01"
        app:layout_constraintEnd_toStartOf="@+id/position_03"/>

    <ImageButton
        android:id="@+id/position_03"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/status"
        app:layout_constraintBottom_toTopOf="@+id/position_13"
        app:layout_constraintStart_toEndOf="@id/position_02"
        app:layout_constraintEnd_toStartOf="@+id/position_04"/>

    <ImageButton
        android:id="@+id/position_04"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/status"
        app:layout_constraintBottom_toTopOf="@+id/position_14"
        app:layout_constraintStart_toEndOf="@id/position_03"
        app:layout_constraintEnd_toStartOf="@id/space"/>

    <ImageButton
        android:id="@+id/position_10"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_00"
        app:layout_constraintBottom_toTopOf="@+id/position_20"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/position_11"/>

    <ImageButton
        android:id="@+id/position_11"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_01"
        app:layout_constraintBottom_toTopOf="@+id/position_21"
        app:layout_constraintStart_toEndOf="@id/position_10"
        app:layout_constraintEnd_toStartOf="@+id/position_12"/>

    <ImageButton
        android:id="@+id/position_12"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_02"
        app:layout_constraintBottom_toTopOf="@+id/position_22"
        app:layout_constraintStart_toEndOf="@id/position_11"
        app:layout_constraintEnd_toStartOf="@+id/position_13"/>

    <ImageButton
        android:id="@+id/position_13"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_03"
        app:layout_constraintBottom_toTopOf="@+id/position_23"
        app:layout_constraintStart_toEndOf="@id/position_12"
        app:layout_constraintEnd_toStartOf="@+id/position_14"/>

    <ImageButton
        android:id="@+id/position_14"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_04"
        app:layout_constraintBottom_toTopOf="@+id/position_24"
        app:layout_constraintStart_toEndOf="@id/position_13"
        app:layout_constraintEnd_toStartOf="@id/space"/>

    <ImageButton
        android:id="@+id/position_20"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_10"
        app:layout_constraintBottom_toTopOf="@+id/position_30"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/position_21"/>

    <ImageButton
        android:id="@+id/position_21"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_11"
        app:layout_constraintBottom_toTopOf="@+id/position_31"
        app:layout_constraintStart_toEndOf="@id/position_20"
        app:layout_constraintEnd_toStartOf="@+id/position_22"/>

    <ImageButton
        android:id="@+id/position_22"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_12"
        app:layout_constraintBottom_toTopOf="@+id/position_32"
        app:layout_constraintStart_toEndOf="@id/position_21"
        app:layout_constraintEnd_toStartOf="@+id/position_23"/>

    <ImageButton
        android:id="@+id/position_23"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_13"
        app:layout_constraintBottom_toTopOf="@+id/position_33"
        app:layout_constraintStart_toEndOf="@id/position_22"
        app:layout_constraintEnd_toStartOf="@+id/position_24"/>

    <ImageButton
        android:id="@+id/position_24"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_14"
        app:layout_constraintBottom_toTopOf="@+id/position_34"
        app:layout_constraintStart_toEndOf="@id/position_23"
        app:layout_constraintEnd_toStartOf="@id/space"/>

    <ImageButton
        android:id="@+id/position_30"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_20"
        app:layout_constraintBottom_toTopOf="@+id/position_40"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/position_31"/>

    <ImageButton
        android:id="@+id/position_31"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_21"
        app:layout_constraintBottom_toTopOf="@+id/position_41"
        app:layout_constraintStart_toEndOf="@id/position_30"
        app:layout_constraintEnd_toStartOf="@+id/position_32"/>

    <ImageButton
        android:id="@+id/position_32"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_22"
        app:layout_constraintBottom_toTopOf="@+id/position_42"
        app:layout_constraintStart_toEndOf="@id/position_31"
        app:layout_constraintEnd_toStartOf="@+id/position_33"/>

    <ImageButton
        android:id="@+id/position_33"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_23"
        app:layout_constraintBottom_toTopOf="@+id/position_43"
        app:layout_constraintStart_toEndOf="@id/position_32"
        app:layout_constraintEnd_toStartOf="@+id/position_34"/>

    <ImageButton
        android:id="@+id/position_34"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_24"
        app:layout_constraintBottom_toTopOf="@+id/position_44"
        app:layout_constraintStart_toEndOf="@id/position_33"
        app:layout_constraintEnd_toStartOf="@id/space"/>

    <ImageButton
        android:id="@+id/position_40"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_30"
        app:layout_constraintBottom_toTopOf="@id/ad_view"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/position_41"/>

    <ImageButton
        android:id="@+id/position_41"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_31"
        app:layout_constraintBottom_toTopOf="@id/ad_view"
        app:layout_constraintStart_toEndOf="@id/position_40"
        app:layout_constraintEnd_toStartOf="@+id/position_42"/>

    <ImageButton
        android:id="@+id/position_42"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_32"
        app:layout_constraintBottom_toTopOf="@id/ad_view"
        app:layout_constraintStart_toEndOf="@id/position_41"
        app:layout_constraintEnd_toStartOf="@+id/position_43"/>

    <ImageButton
        android:id="@+id/position_43"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_33"
        app:layout_constraintBottom_toTopOf="@id/ad_view"
        app:layout_constraintStart_toEndOf="@id/position_42"
        app:layout_constraintEnd_toStartOf="@+id/position_44"/>

    <ImageButton
        android:id="@+id/position_44"
        style="@style/button"
        app:layout_constraintTop_toBottomOf="@id/position_34"
        app:layout_constraintBottom_toTopOf="@id/ad_view"
        app:layout_constraintStart_toEndOf="@id/position_43"
        app:layout_constraintEnd_toStartOf="@id/space"/>

    <ImageView
        android:id="@+id/space"
        android:src="@drawable/space"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/position_44"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/ad_view"
        android:scaleType="centerInside"
        app:layout_constraintHorizontal_weight="3"
        android:visibility="gone" />

    <com.google.android.gms.ads.AdView
        android:id="@+id/ad_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        app:adSize="BANNER"
        app:adUnitId="ca-app-pub-1316488884400350/3260392156"
        app:layout_constraintTop_toBottomOf="@id/position_44"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>